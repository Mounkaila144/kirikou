{% extends 'base.html.twig' %}

{% block title %}Achat{% endblock %}

{% block body %}
    <h1>Achat</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ achat.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ achat.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ achat.prenom }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ achat.adresse }}</td>
            </tr>
            <tr>
                <th>Numero</th>
                <td>{{ achat.numero }}</td>
            </tr>
            <tr>
                <th>Abonnement</th>
                <td>{{ achat.abonnement }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_achat_index') }}">back to list</a>

    <a href="{{ path('app_achat_edit', {'id': achat.id}) }}">edit</a>

    {{ include('achat/_delete_form.html.twig') }}
{% endblock %}
